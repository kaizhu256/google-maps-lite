{
    "basePath": "/",
    "host": "maps.googleapis.com",
    "info": {
        "description": "this zero-dependency package will provide an api for google-maps, with a working demo",
        "title": "google-maps-lite",
        "version": "2017.10.1",
        "x-homepage": "https://github.com/kaizhu256/node-google-maps-lite"
    },
    "paths": {
        "/maps/api/directions/json": {
            "get": {
                "operationId": "mapsApiDirectionsJson",
                "parameters": [
                    {
                        "description": "Your application's API key. This key identifies your application for purposes of quota management. https://developers.google.com/maps/documentation/directions/get-api-key",
                        "in": "query",
                        "name": "key",
                        "type": "string",
                        "x-example": "xxxxxxxx"
                    },
                    {
                        "description": "The language in which to return results. https://developers.google.com/maps/faq#languagesupport",
